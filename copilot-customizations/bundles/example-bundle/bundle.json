{
  "name": "example-bundle",
  "version": "1.0.0",
  "description": "Example bundle demonstrating the bundle structure with copilot customizations",
  "copilotInstructions": {
    "type": "inline",
    "path": "copilot-instructions.md",
    "description": "Bundle-specific instructions on how to use the agents and skills together"
  },
  "dependencies": {
    "agents": [
      {
        "name": "skill-builder",
        "type": "reference",
        "source": "agents/latest/skill-builder.agent.md",
        "version": "latest",
        "description": "Expert in creating and modifying GitHub Copilot skills"
      }
    ],
    "prompts": [
      {
        "name": "git",
        "type": "reference",
        "source": "prompts/latest/git.prompt.md",
        "version": "latest",
        "description": "Git operations prompt"
      },
      {
        "name": "custom-prompt",
        "type": "inline",
        "path": "prompts/custom.prompt.md",
        "description": "Bundle-specific custom prompt only relevant in this bundle"
      }
    ],
    "skills": [
      {
        "name": "test-driven-development",
        "type": "reference",
        "source": "skills/latest/test-driven-development",
        "version": "latest",
        "description": "TDD workflow enforcement"
      }
    ],
    "instructions": []
  },
  "metadata": {
    "author": "DJ2695",
    "created": "2026-02-01",
    "tags": ["example", "demo"],
    "compatibleCopilotVersions": ["*"]
  }
}
